  "bugs": {
    "url": "https://github.com/diego-rangel/ngx-ui-hero/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "release": "ng build --prod ngx-ui-hero && npm run package && npm run generate_assets",
    "package": "ng build --prod --base-href /ngx-ui-hero/ --deploy-url /ngx-ui-hero/ && npm run generate_docs",
    "generate_docs": "if exist docs RMDIR /S /Q docs && mkdir docs && xcopy dist\\ngx-ui-hero-app docs /E /C /H /R /K /Y",
    "generate_assets": "xcopy dist\\ngx-ui-hero-app\\assets\\css dist\\ngx-ui-hero /E /C /H /R /K /Y"
  },
  "private": true,
