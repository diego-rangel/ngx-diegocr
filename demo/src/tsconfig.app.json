{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "module": "es2015",
    "types": [],
    "paths": {
      "@angular/*": ["../node_modules/@angular/*"],
      "bootstrap": ["../node_modules/bootstrap"],
      "font-awesome": ["../node_modules/font-awesome"],
      "jquery": ["../node_modules/jquery"],
      "ng2-currency-mask": ["../node_modules/ng2-currency-mask"],
      "ngx-toastr": ["../node_modules/ngx-toastr"],
      "popper.js": ["../node_modules/popper.js"],
      "rxjs": ["../node_modules/rxjs"],
      "rxjs-compat": ["../node_modules/rxjs-compat"],
      "sweetalert2": ["../node_modules/sweetalert2"]
    }
  },
  "exclude": [
    "testing/*",
    "test.ts",
    "**/*.spec.ts"
  ]
}
